{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": false,
    "experimentalDecorators": false,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "types" : ["node"],
    "strictNullChecks": true
  },
  "exclude": [
    "node_modules/",
    ".vscode/"
  ]
}
